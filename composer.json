{
    "name": "juanchosl/simplecache",
    "description": "A small collection of read/write for multiples cache systems using PSR-16",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/JuanchoSL/SimpleCache",
    "keywords": [
        "cache",
        "memcache cache",
        "memcached cache",
        "redis cache",
        "session cache",
        "file cache",
        "process cache",
        "psr-16",
        "simple cache"
    ],
    "authors": [
        {
            "name": "Juan SÃ¡nchez Lecegui",
            "email": "Juan.Sanchez@tecnicosweb.com",
            "homepage": "https://www.linkedin.com/in/juansanchezlecegui/",
            "role": "Developer"
        }
    ],
    "suggest": {
        "ext-redis": "*",
        "ext-memcache": "*",
        "ext-memcached": "*"
    },
    "require": {
        "php": "^7.2 || ^8.0",
        "psr/log": "3.0.*",
        "psr/simple-cache": "3.0.*",
        "psr/cache": "3.0.*",
        "juanchosl/validators": "1.0.*",
        "juanchosl/exceptions": "1.0.*"
    },
    "require-dev": {
        "juanchosl/logger": "1.1.*",
        "phpunit/phpunit": "9.6.*",
        "phpstan/phpstan": "1.12.*"
    },
    "autoload": {
        "psr-4": {
            "JuanchoSL\\SimpleCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JuanchoSL\\SimpleCache\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests": "vendor/bin/phpunit --configuration phpunit.xml tests",
        "phpstan": "vendor/bin/phpstan analyse -c phpstan.neon"
    }
}